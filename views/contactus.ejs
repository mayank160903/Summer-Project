<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css"
    integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css2?family=Cabin&family=Lobster&family=Montserrat:ital,wght@1,100&family=Oswald:wght@300;400&family=Oxygen:wght@300;700&family=Prompt:wght@600&family=Quicksand:wght@300&family=Righteous&family=Secular+One&family=Tilt+Prism&display=swap" rel="stylesheet">
   

  <link rel="stylesheet" href="css/contactus.css">
  <script src="js/contactus.js"></script>
  <title>Contact Us</title>

</head>
<% if (auth == true) { %>
  <%- include('login-header',{user:user}); %>
<%}%>

  <% if(auth != true){ %>
  <%- include('header'); %>
<%}%>
 
<div class="wholepage contact-bg">
<body class="body-container-wrapper body-container contact-bg">
   
    <div class="section">
      <div class="container-fluid">
        <div class="mainformcont">
          <div class="container">
            <div class="mainform d-flex flex-row flex-wrap">

              <div class="formblock col">
                <div class="fhv">
                  <span class="formheader">
                    <h1>Get in Touch With Us</h1>
                  </span>
                </div>

                <div class="mt-5 container-fluid-wrapper">
                  <div class="container">
                    <div class="form-col-container row row-cols-2">

                      <div class="col-2 forminnerblock ">
                        <img class="img-fluid w-75 itemimage"
                          src="https://www.peoplemetrics.com/hubfs/Website/email_1-blue-bottom.svg" alt="email"
                          loading="">
                      </div>

                      <div class="col-10 formtxt "><a>contact@guitar365.com</a>
                      </div>

                      <div class="col-2 forminnerblock ">
                        <img class="img-fluid w-75 itemimage"
                          src="https://www.peoplemetrics.com/hubfs/Website/phone_1-blue-bottom.svg" alt="phone"
                          loading="">
                      </div>

                      <div class="col-10 formtxt "> <a>+91 8076779705</a>
                      </div>

                      <div class="col-2 forminnerblock ">
                        <img class="img-fluid w-75 itemimage"
                          src="https://www.peoplemetrics.com/hubfs/Website/pin-blue-bottom.svg" alt="email" loading="">
                      </div>

                      <div class="col-10 formtxt"> <a href="https://goo.gl/maps/btQAbi3aqZfHdSz39">BH1 IIIT SriCity,
                          Chitoor</a>
                      </div>
                    </div>
                  </div>
                </div>





              </div>
              <div class="formb2 mt-5">
                <div class="contact-wrapper">



                  <form id="contact-form" type="submit" action="/connect" actionclass="form-horizontal" role="form"
                    method="POST">

                    <div class="form-group row justify-content-between " style="width:100%">
                      <div class="col-sm-6" style="padding-right: 0px;padding-left: 30px;">
                        <input type="text" class="form-control" id="name" placeholder=" FIRST NAME" name="fname"
                          value="" required>
                      </div>

                      <div class="col-sm-6" style="padding-left: 28px; padding-right: 0px;">
                        <input type="text" class="form-control" id="name2" placeholder="LAST NAME" name="lname" value=""
                          required>
                      </div>

                    </div>

                    <div class="form-group">
                      <div class="col-sm-12">
                        <input type="email" class="form-control " id="email" placeholder="EMAIL" name="email" value=""
                          required>
                      </div>
                    </div>
                    <div class="col-sm-12">
                      <textarea class="form-control " rows="10" placeholder="MESSAGE" name="message" required></textarea>
                    </div>

                    <button class="btn btn-primary send-button" id="submit" type="submit" value="SEND">
                      <span class="send-text">SEND</span>

                    </button>

                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div>
      <br>
      <br>
      <br>
      <br>
      <br>
    </div>
    </div>
    </div>
    </div>
  </div>
    <%- include('footer'); -%>




      <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
        integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n"
        crossorigin="anonymous"></script>
      <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js"
        integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
        crossorigin="anonymous"></script>

</body>

</html>
